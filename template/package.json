{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "1.0.0",
  "author": "Making Waves",
  "license": "All Rights Reserved",
  "private": true,
  "scripts": {
    "postinstall": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack",
    "dev:watch": "cross-env NODE_ENV=development webpack -w",
    "dev:stats": "cross-env NODE_ENV=development webpack && source-map-explorer ./Assets/Bundled/main.bundle.js",
    {{#lint}}
    "lint": "eslint --ext .js,.vue assets",
    "lint:fix": "npm run lint -- --fix",
    {{/lint}}
    {{#unit}}
    "unit": "jest --config test/unit/jest.conf.js --coverage --verbose",
    {{/unit}}
    {{#styleguide}}
    "styleguide": "cross-env NODE_ENV=production webpack && stylemark -i . -o styleguide -c stylemark.yml",
    {{/styleguide}}
    "build": "cross-env NODE_ENV=production webpack",
    "build:stats": "cross-env NODE_ENV=production webpack --devtool source-map && source-map-explorer ./Assets/Bundled/main.bundle.js"
  },
  "dependencies": {
    {{#vue}}
    "vue": "^2.5.11"{{#router}},
    "vue-router": "^3.0.1"{{/router}}{{#vuex}},
    "vuex": "^3.0.1"
    {{/vuex}}
    {{/vue}}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "@babel/core": "^7.1.2",
    "babel-loader": "^8.0.4",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-stage-3": "^7.0.0",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "clean-webpack-plugin": "^0.1.19",
    {{#unit}}
    "babel-jest": "^23.4.0",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "jest": "^23.4.1",
    "jest-serializer-vue": "^2.0.2",
    {{#vue}}
    "vue-jest": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.21",
    {{/vue}}
    {{/unit}}
    {{#lint}}
    "babel-eslint": "^10.0.1",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    {{/lint}}
    "mini-css-extract-plugin": "^0.4.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.0.0",
    "import-glob-loader": "^1.1.0",
    "less": "^3.6.0",
    "less-loader": "^4.1.0",
    "postcss-color-rgba-fallback": "^3.0.0",
    "postcss-less": "^3.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-pxtorem": "^4.0.1",
    {{#vue}}
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.4.4",
    {{/vue}}
    {{#styleguide}}
    "stylemark": "^3.0.2",
    {{/styleguide}}
    "svg-sprite-loader": "^3.8.0",
    "svgo-loader": "^2.1.0",
    "source-map-explorer": "^1.5.0",
    "webpack": "^4.16.3",
    "webpack-build-notifier": "^0.1.28",
    "webpack-cli": "^3.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "-vs-binding": {
      "ProjectOpened": [
          "dev:watch"
      ]
  }
}
